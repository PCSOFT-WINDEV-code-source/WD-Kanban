     name : DragCarte_Drop
     procedure_id : 1401927013895343343
     type_code : 14
     code : |1+
      procédure DragCarte_Drop(Carte ) //est un kbCarte)
      
      // exécution du traitement d'ajout et annulation s'il renvoie FAUX
      SI ChampKanban.ExécuteTraitement( trtSuppressionCarteKanban, Carte ) = Faux ALORS
      	// on annule
      	RETOUR
      FIN
      
      // Suppression de la carte
      nIndice est entier =  Carte.Indice
      ListeCarte.SupprimeCarte(nIndice) 
      carteCopie est un kbCarte = Carte	
      // On affiche un toast pour permettre d'annuler la suppression de la carte
      toast est un Toast
      toast.CadrageVertical = cvMilieu
      
      <SI CibleExécution = Android OU CibleExécution = iOS>		
      	toast..Texte = <§R13502efd0a226952¤fr-FR¤La carte a été supprimée.¤en-GB¤¤es-ES¤§>
      <sinon>	
      	toast..Titre = Carte..Titre
      	toast..Texte = <§R13501804036acbb1¤fr-FR¤a été supprimé¤en-GB¤¤es-ES¤§>
      	toast..Superposable= faux
      	toast..FermeAuClic = vrai
      <FIN>
      
      toast..DuréeAffichage = 5s
      toast..Bouton[1]..Libellé = <§R134ed795566f1863¤fr-FR¤Annuler¤en-GB¤¤es-ES¤§>
      toast..Bouton[1]..ActionClic = () => { ListeCarte.InsèreCarte( nIndice, carteCopie ) }
      toast.Affiche()
     type : 458752
