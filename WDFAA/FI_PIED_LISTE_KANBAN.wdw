            Carte est une kbcarte
            
            // Si on a validé la saisie
            si fen_kbcarte.Ouvre( Carte ) = vrai ALORS
            
            	// Exécute le traitement d'ajout de carte
            	si ChampKanban.ExécuteTraitement( trtAjoutCarteKanban, Carte, ListeCarte ) = faux ALORS
            		retour
            	FIN
            	
            	// Ajoute la carte dans la liste
            	nIndice est un entier = ListeCarte.AjouteCarte( Carte )
            	si nIndice > 0 ALORS
            		
            		// On se positionne sur la carte ajoutée		
            		ListeCarte.PositionListe( ListeCarte.Carte[ nIndice ] )
            	FIN
            FIN
